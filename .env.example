# API Keys
GOOGLE_API_KEY=your_api_key_here

# Crawler Settings
START_URL=https://www.example.edu
ALLOWED_DOMAIN=example.edu
MAX_CONCURRENT=20
MAX_DEPTH=30
MAX_RETRIES=2
MAX_URLS=500000
LOG_LEVEL=info

# Timeout settings (in milliseconds)
PAGE_TIMEOUT=30000
NAVIGATION_TIMEOUT=30000
WAIT_UNTIL=networkidle0

# AI Analysis settings
AI_REQUEST_DELAY=2000
AI_MAX_RETRIES=3
AI_QUEUE_MAX_SIZE=1000

# Run mode settings
CRAWLER_MODE=true
AI_MODE=false
STOP_WHEN_EMPTY=true 